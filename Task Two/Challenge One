public class Fibonacci {
    static int fib(int n)
    {
        if (n <= 1)
            return n;
        return fib(n - 1) + fib(n - 2);
    }
  
    public static void main(String args[])
    {
        int n = 17;
        System.out.println("The fibonacci number " + n + " in the fibonacci sequence is " + fib(n));
    }
}
